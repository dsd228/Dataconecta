<svg width="600" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8f9fa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e9ecef;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="600" height="400" fill="url(#bg)"/>
  
  <!-- Title -->
  <text x="300" y="30" font-family="Inter, sans-serif" font-size="18" font-weight="600" text-anchor="middle" fill="#212529">Rendimiento del Modelo ML</text>
  
  <!-- Chart area -->
  <rect x="80" y="60" width="440" height="280" fill="#ffffff" stroke="#dee2e6" stroke-width="1"/>
  
  <!-- Precision bar -->
  <rect x="120" y="100" width="80" height="30" fill="#198754" opacity="0.8"/>
  <text x="110" y="120" font-family="Inter, sans-serif" font-size="12" text-anchor="end" fill="#212529">Precisi√≥n</text>
  <text x="210" y="120" font-family="Inter, sans-serif" font-size="12" fill="#212529">94.2%</text>
  
  <!-- Recall bar -->
  <rect x="120" y="150" width="75" height="30" fill="#0dcaf0" opacity="0.8"/>
  <text x="110" y="170" font-family="Inter, sans-serif" font-size="12" text-anchor="end" fill="#212529">Recall</text>
  <text x="205" y="170" font-family="Inter, sans-serif" font-size="12" fill="#212529">91.8%</text>
  
  <!-- F1-Score bar -->
  <rect x="120" y="200" width="77" height="30" fill="#6f42c1" opacity="0.8"/>
  <text x="110" y="220" font-family="Inter, sans-serif" font-size="12" text-anchor="end" fill="#212529">F1-Score</text>
  <text x="207" y="220" font-family="Inter, sans-serif" font-size="12" fill="#212529">93.0%</text>
  
  <!-- Accuracy bar -->
  <rect x="120" y="250" width="82" height="30" fill="#fd7e14" opacity="0.8"/>
  <text x="110" y="270" font-family="Inter, sans-serif" font-size="12" text-anchor="end" fill="#212529">Exactitud</text>
  <text x="212" y="270" font-family="Inter, sans-serif" font-size="12" fill="#212529">95.1%</text>
  
  <!-- Scale lines -->
  <line x1="120" y1="90" x2="120" y2="290" stroke="#dee2e6" stroke-width="1"/>
  <line x1="200" y1="90" x2="200" y2="290" stroke="#f8f9fa" stroke-width="1"/>
  <line x1="280" y1="90" x2="280" y2="290" stroke="#f8f9fa" stroke-width="1"/>
  <line x1="360" y1="90" x2="360" y2="290" stroke="#f8f9fa" stroke-width="1"/>
  <line x1="440" y1="90" x2="440" y2="290" stroke="#f8f9fa" stroke-width="1"/>
  
  <!-- Scale labels -->
  <text x="120" y="305" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">0%</text>
  <text x="200" y="305" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">25%</text>
  <text x="280" y="305" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">50%</text>
  <text x="360" y="305" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">75%</text>
  <text x="440" y="305" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">100%</text>
  
  <!-- ROC Curve in corner -->
  <text x="350" y="110" font-family="Inter, sans-serif" font-size="14" font-weight="600" fill="#212529">Curva ROC</text>
  <rect x="340" y="120" width="140" height="140" fill="#ffffff" stroke="#dee2e6" stroke-width="1"/>
  
  <!-- ROC curve line -->
  <path d="M 350 250 Q 380 220 420 180 Q 450 150 470 130" stroke="#0d6efd" stroke-width="3" fill="none"/>
  <!-- Diagonal reference line -->
  <line x1="350" y1="250" x2="470" y2="130" stroke="#dee2e6" stroke-width="1" stroke-dasharray="5,5"/>
  
  <!-- ROC labels -->
  <text x="345" y="270" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">0</text>
  <text x="465" y="270" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">1</text>
  <text x="335" y="135" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">1</text>
  <text x="335" y="255" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">0</text>
  
  <!-- AUC text -->
  <text x="410" y="200" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#0d6efd" font-weight="600">AUC = 0.97</text>
</svg>
<svg width="600" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8f9fa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e9ecef;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="cardGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f8f9fa;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="600" height="400" fill="url(#bg)"/>
  
  <!-- Title -->
  <text x="300" y="30" font-family="Inter, sans-serif" font-size="18" font-weight="600" text-anchor="middle" fill="#212529">Dashboard Interactivo - DataConecta</text>
  
  <!-- Main dashboard container -->
  <rect x="20" y="50" width="560" height="330" fill="url(#cardGrad)" stroke="#dee2e6" stroke-width="1" rx="10"/>
  
  <!-- Top metrics row -->
  <!-- Total Transactions -->
  <rect x="40" y="70" width="120" height="60" fill="#0d6efd" opacity="0.1" stroke="#0d6efd" stroke-width="1" rx="5"/>
  <text x="100" y="90" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">Total Transacciones</text>
  <text x="100" y="110" font-family="Inter, sans-serif" font-size="16" font-weight="600" text-anchor="middle" fill="#0d6efd">124,567</text>
  <text x="100" y="125" font-family="Inter, sans-serif" font-size="8" text-anchor="middle" fill="#198754">↑ +5.2% vs ayer</text>
  
  <!-- Fraud Detection -->
  <rect x="180" y="70" width="120" height="60" fill="#dc3545" opacity="0.1" stroke="#dc3545" stroke-width="1" rx="5"/>
  <text x="240" y="90" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">Fraudes Detectados</text>
  <text x="240" y="110" font-family="Inter, sans-serif" font-size="16" font-weight="600" text-anchor="middle" fill="#dc3545">1,247</text>
  <text x="240" y="125" font-family="Inter, sans-serif" font-size="8" text-anchor="middle" fill="#dc3545">↑ +2.1% vs ayer</text>
  
  <!-- Success Rate -->
  <rect x="320" y="70" width="120" height="60" fill="#198754" opacity="0.1" stroke="#198754" stroke-width="1" rx="5"/>
  <text x="380" y="90" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">Tasa de Éxito</text>
  <text x="380" y="110" font-family="Inter, sans-serif" font-size="16" font-weight="600" text-anchor="middle" fill="#198754">94.2%</text>
  <text x="380" y="125" font-family="Inter, sans-serif" font-size="8" text-anchor="middle" fill="#198754">↑ +0.8% vs ayer</text>
  
  <!-- Response Time -->
  <rect x="460" y="70" width="120" height="60" fill="#ffc107" opacity="0.1" stroke="#ffc107" stroke-width="1" rx="5"/>
  <text x="520" y="90" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">Tiempo Respuesta</text>
  <text x="520" y="110" font-family="Inter, sans-serif" font-size="16" font-weight="600" text-anchor="middle" fill="#ffc107">1.2s</text>
  <text x="520" y="125" font-family="Inter, sans-serif" font-size="8" text-anchor="middle" fill="#198754">↓ -0.3s vs ayer</text>
  
  <!-- Real-time chart area -->
  <rect x="40" y="150" width="280" height="120" fill="#ffffff" stroke="#dee2e6" stroke-width="1" rx="5"/>
  <text x="180" y="170" font-family="Inter, sans-serif" font-size="12" font-weight="600" text-anchor="middle" fill="#212529">Transacciones en Tiempo Real</text>
  
  <!-- Mini line chart -->
  <path d="M 60 240 L 80 220 L 100 230 L 120 210 L 140 225 L 160 205 L 180 215 L 200 195 L 220 210 L 240 190 L 260 200 L 280 180 L 300 190" 
        stroke="#0d6efd" stroke-width="2" fill="none"/>
  <path d="M 60 250 L 80 245 L 100 248 L 120 240 L 140 242 L 160 238 L 180 245 L 200 235 L 220 240 L 240 230 L 260 235 L 280 225 L 300 230" 
        stroke="#dc3545" stroke-width="2" fill="none"/>
  
  <!-- Chart legend -->
  <line x1="50" y1="255" x2="60" y2="255" stroke="#0d6efd" stroke-width="2"/>
  <text x="65" y="259" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">Normales</text>
  <line x1="120" y1="255" x2="130" y2="255" stroke="#dc3545" stroke-width="2"/>
  <text x="135" y="259" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">Fraudes</text>
  
  <!-- Alert panel -->
  <rect x="340" y="150" width="240" height="120" fill="#ffffff" stroke="#dee2e6" stroke-width="1" rx="5"/>
  <text x="460" y="170" font-family="Inter, sans-serif" font-size="12" font-weight="600" text-anchor="middle" fill="#212529">Alertas Recientes</text>
  
  <!-- Alert items -->
  <circle cx="355" cy="185" r="3" fill="#dc3545"/>
  <text x="365" y="189" font-family="Inter, sans-serif" font-size="9" fill="#212529">Transacción sospechosa $5,000</text>
  <text x="565" y="189" font-family="Inter, sans-serif" font-size="8" text-anchor="end" fill="#6c757d">14:23</text>
  
  <circle cx="355" cy="205" r="3" fill="#ffc107"/>
  <text x="365" y="209" font-family="Inter, sans-serif" font-size="9" fill="#212529">Patrón anómalo detectado</text>
  <text x="565" y="209" font-family="Inter, sans-serif" font-size="8" text-anchor="end" fill="#6c757d">14:15</text>
  
  <circle cx="355" cy="225" r="3" fill="#dc3545"/>
  <text x="365" y="229" font-family="Inter, sans-serif" font-size="9" fill="#212529">Múltiples intentos fallidos</text>
  <text x="565" y="229" font-family="Inter, sans-serif" font-size="8" text-anchor="end" fill="#6c757d">14:08</text>
  
  <circle cx="355" cy="245" r="3" fill="#198754"/>
  <text x="365" y="249" font-family="Inter, sans-serif" font-size="9" fill="#212529">Sistema funcionando normal</text>
  <text x="565" y="249" font-family="Inter, sans-serif" font-size="8" text-anchor="end" fill="#6c757d">14:00</text>
  
  <!-- Model status panel -->
  <rect x="40" y="290" width="540" height="70" fill="#ffffff" stroke="#dee2e6" stroke-width="1" rx="5"/>
  <text x="310" y="310" font-family="Inter, sans-serif" font-size="12" font-weight="600" text-anchor="middle" fill="#212529">Estado del Modelo ML</text>
  
  <!-- Status indicators -->
  <circle cx="80" cy="330" r="5" fill="#198754"/>
  <text x="95" y="335" font-family="Inter, sans-serif" font-size="10" fill="#212529">Modelo Principal</text>
  <text x="95" y="345" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">Activo - 99.2% uptime</text>
  
  <circle cx="220" cy="330" r="5" fill="#198754"/>
  <text x="235" y="335" font-family="Inter, sans-serif" font-size="10" fill="#212529">Análisis Tiempo Real</text>
  <text x="235" y="345" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">Procesando - 1,200 TPS</text>
  
  <circle cx="380" cy="330" r="5" fill="#ffc107"/>
  <text x="395" y="335" font-family="Inter, sans-serif" font-size="10" fill="#212529">Modelo Backup</text>
  <text x="395" y="345" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">Standby - Listo</text>
  
  <circle cx="510" cy="330" r="5" fill="#198754"/>
  <text x="525" y="335" font-family="Inter, sans-serif" font-size="10" fill="#212529">Base de Datos</text>
  <text x="525" y="345" font-family="Inter, sans-serif" font-size="8" fill="#6c757d">Sincronizada - 2ms latencia</text>
</svg>
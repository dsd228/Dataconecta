<svg width="600" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8f9fa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e9ecef;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="600" height="400" fill="url(#bg)"/>
  
  <!-- Title -->
  <text x="300" y="30" font-family="Inter, sans-serif" font-size="18" font-weight="600" text-anchor="middle" fill="#212529">Patrones de Transacciones</text>
  
  <!-- Chart area -->
  <rect x="80" y="60" width="440" height="280" fill="#ffffff" stroke="#dee2e6" stroke-width="1"/>
  
  <!-- Scatter plot - Normal transactions (green) -->
  <circle cx="150" cy="120" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="170" cy="140" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="190" cy="160" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="210" cy="180" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="230" cy="200" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="250" cy="220" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="270" cy="240" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="290" cy="260" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="160" cy="110" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="180" cy="130" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="200" cy="150" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="220" cy="170" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="240" cy="190" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="260" cy="210" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="280" cy="230" r="3" fill="#198754" opacity="0.7"/>
  <circle cx="300" cy="250" r="3" fill="#198754" opacity="0.7"/>
  
  <!-- More normal pattern scatter -->
  <circle cx="155" cy="125" r="2" fill="#198754" opacity="0.5"/>
  <circle cx="175" cy="145" r="2" fill="#198754" opacity="0.5"/>
  <circle cx="195" cy="165" r="2" fill="#198754" opacity="0.5"/>
  <circle cx="215" cy="185" r="2" fill="#198754" opacity="0.5"/>
  <circle cx="235" cy="205" r="2" fill="#198754" opacity="0.5"/>
  <circle cx="255" cy="225" r="2" fill="#198754" opacity="0.5"/>
  <circle cx="275" cy="245" r="2" fill="#198754" opacity="0.5"/>
  <circle cx="295" cy="265" r="2" fill="#198754" opacity="0.5"/>
  
  <!-- Fraudulent transactions (red outliers) -->
  <circle cx="320" cy="100" r="4" fill="#dc3545" opacity="0.8"/>
  <circle cx="450" cy="90" r="4" fill="#dc3545" opacity="0.8"/>
  <circle cx="380" cy="120" r="4" fill="#dc3545" opacity="0.8"/>
  <circle cx="420" cy="110" r="4" fill="#dc3545" opacity="0.8"/>
  <circle cx="350" cy="280" r="4" fill="#dc3545" opacity="0.8"/>
  <circle cx="480" cy="290" r="4" fill="#dc3545" opacity="0.8"/>
  <circle cx="410" cy="300" r="4" fill="#dc3545" opacity="0.8"/>
  <circle cx="140" cy="90" r="4" fill="#dc3545" opacity="0.8"/>
  <circle cx="120" cy="310" r="4" fill="#dc3545" opacity="0.8"/>
  
  <!-- Trend line for normal pattern -->
  <line x1="130" y1="100" x2="310" y2="280" stroke="#198754" stroke-width="2" opacity="0.6"/>
  
  <!-- Axes -->
  <line x1="80" y1="340" x2="520" y2="340" stroke="#dee2e6" stroke-width="1"/>
  <line x1="80" y1="60" x2="80" y2="340" stroke="#dee2e6" stroke-width="1"/>
  
  <!-- X-axis labels -->
  <text x="150" y="355" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">Bajo</text>
  <text x="250" y="355" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">Medio</text>
  <text x="350" y="355" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">Alto</text>
  <text x="450" y="355" font-family="Inter, sans-serif" font-size="10" text-anchor="middle" fill="#6c757d">Muy Alto</text>
  <text x="300" y="380" font-family="Inter, sans-serif" font-size="12" text-anchor="middle" fill="#6c757d">Monto de Transacción</text>
  
  <!-- Y-axis labels -->
  <text x="70" y="315" font-family="Inter, sans-serif" font-size="10" text-anchor="end" fill="#6c757d">0-6</text>
  <text x="70" y="265" font-family="Inter, sans-serif" font-size="10" text-anchor="end" fill="#6c757d">6-12</text>
  <text x="70" y="215" font-family="Inter, sans-serif" font-size="10" text-anchor="end" fill="#6c757d">12-18</text>
  <text x="70" y="165" font-family="Inter, sans-serif" font-size="10" text-anchor="end" fill="#6c757d">18-24</text>
  <text x="70" y="115" font-family="Inter, sans-serif" font-size="10" text-anchor="end" fill="#6c757d">Madrugada</text>
  
  <!-- Y-axis title (rotated) -->
  <text x="25" y="200" font-family="Inter, sans-serif" font-size="12" text-anchor="middle" fill="#6c757d" transform="rotate(-90 25 200)">Hora del Día</text>
  
  <!-- Legend -->
  <circle cx="420" y="80" r="4" fill="#198754" opacity="0.7"/>
  <text x="430" y="84" font-family="Inter, sans-serif" font-size="12" fill="#212529">Transacciones Normales</text>
  
  <circle cx="420" y="100" r="4" fill="#dc3545" opacity="0.8"/>
  <text x="430" y="104" font-family="Inter, sans-serif" font-size="12" fill="#212529">Transacciones Fraudulentas</text>
  
  <!-- Anomaly zones -->
  <ellipse cx="400" cy="100" rx="80" ry="25" fill="none" stroke="#dc3545" stroke-width="2" stroke-dasharray="5,5" opacity="0.5"/>
  <ellipse cx="400" cy="295" rx="80" ry="25" fill="none" stroke="#dc3545" stroke-width="2" stroke-dasharray="5,5" opacity="0.5"/>
  <ellipse cx="130" cy="200" rx="30" ry="120" fill="none" stroke="#dc3545" stroke-width="2" stroke-dasharray="5,5" opacity="0.5"/>
</svg>